# Copy to .env and fill in values. Used by docker-compose and local dev.

# Use your Neon PostgreSQL connection string with sslmode=require
# Example format:
# postgresql://<user>:<password>@<neon-host>/<db>?sslmode=require
DATABASE_URL=postgresql://USER:PASSWORD@HOST:PORT/DB?sslmode=require

# Django settings
DEBUG=True
DJANGO_SECRET_KEY=change-me

# Cloudinary (optional but recommended for media)
CLOUDINARY_CLOUD_NAME=
CLOUDINARY_API_KEY=
CLOUDINARY_API_SECRET=
USE_CLOUDINARY=True
